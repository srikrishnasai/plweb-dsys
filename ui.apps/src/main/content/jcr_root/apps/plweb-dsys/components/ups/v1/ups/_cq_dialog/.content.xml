<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Ups"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter Title"
                                        name="./title"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Enter description"
                                        name="./description"/>
                                    <alignments
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Select text alignments"
                                        name="./alignments">
                                        <items jcr:primaryType="nt:unstructured">
                                            <left
                                                jcr:primaryType="nt:unstructured"
                                                text="Left"
                                                value="left"/>
                                            <center
                                                jcr:primaryType="nt:unstructured"
                                                text="Center"
                                                value="center"/>
                                        </items>
                                    </alignments>
                                    <numberOfSection
                                        granite:class="ups-show-hide-tabs"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="Choose the number of sections"
                                        fieldLabel="Number Of Sections"
                                        name="./numberOfSections">
                                        <items jcr:primaryType="nt:unstructured">
                                            <one
                                                jcr:primaryType="nt:unstructured"
                                                text="One"
                                                value="1"/>
                                            <two
                                                jcr:primaryType="nt:unstructured"
                                                text="Two"
                                                value="2"/>
                                            <three
                                                jcr:primaryType="nt:unstructured"
                                                text="Three"
                                                value="3"/>
                                            <four
                                                jcr:primaryType="nt:unstructured"
                                                text="Four"
                                                value="4"/>
                                        </items>
                                    </numberOfSection>
                                </items>
                            </column>
                        </items>
                    </general>
                    <section1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section1Image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./section1img"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./section1image"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <section1Header
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section1 header text"
                                        fieldLabel="Section1 Header"
                                        name="./section1header"/>
                                    <section1Description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section1 description text"
                                        fieldLabel="Section1 Description"
                                        name="./section1description"/>
                                    <section1SuperScript
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter superscript for description."
                                        fieldLabel="Section1 Superscript"
                                        name="./section1SuperScript"/>
                                </items>
                            </column>
                        </items>
                    </section1>
                    <section2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section2Image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./section2img"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./section2image"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <section2Header
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section2 header text"
                                        fieldLabel="Section2 Header"
                                        name="./section2header"/>
                                    <section2Description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section2 description text"
                                        fieldLabel="Section2 Description"
                                        name="./section2description"/>
                                    <section2SuperScript
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter superscript for description."
                                        fieldLabel="Section2 Superscript"
                                        name="./section2SuperScript"/>
                                </items>
                            </column>
                        </items>
                    </section2>
                    <section3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section3Image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./section3img"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./section3image"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <section3Header
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section3 header text"
                                        fieldLabel="Section3 Header"
                                        name="./section3header"/>
                                    <section3Description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section3 description text"
                                        fieldLabel="Section3 Description"
                                        name="./section3description"/>
                                    <section3SuperScript
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter superscript for description."
                                        fieldLabel="Section3 Superscript"
                                        name="./section3SuperScript"/>
                                </items>
                            </column>
                        </items>
                    </section3>
                    <section4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section4"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section4Image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image"
                                        fileNameParameter="./section4img"
                                        mimeTypes="[image]"
                                        multiple="{Boolean}false"
                                        name="./section4image"
                                        title="Upload Image Asset"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <section4Header
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section4 header text"
                                        fieldLabel="Section4 Header"
                                        name="./section4header"/>
                                    <section4Description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter section4 description text"
                                        fieldLabel="Section4 Description"
                                        name="./section4description"/>
                                    <section4SuperScript
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter superscript for description."
                                        fieldLabel="Section4 Superscript"
                                        name="./section4SuperScript"/>
                                </items>
                            </column>
                        </items>
                    </section4>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
