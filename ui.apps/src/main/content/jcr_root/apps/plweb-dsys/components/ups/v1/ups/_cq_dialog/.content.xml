<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Ups"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Description"
                                                name="./description"/>
                                            <alignments
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Text Alignments"
                                                name="./alignments">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <left
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Left"
                                                        value="left"/>
                                                    <center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Center"
                                                        value="center"/>
                                                </items>
                                            </alignments>
                                            <numberOfSection
                                                granite:class="ups-show-hide-tabs"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyOption="{Boolean}true"
                                                fieldDescription="Choose the number of sections"
                                                fieldLabel="Number Of Sections"
                                                name="./numberOfSections">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <one
                                                        jcr:primaryType="nt:unstructured"
                                                        text="One"
                                                        value="1"/>
                                                    <two
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Two"
                                                        value="2"/>
                                                    <three
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Three"
                                                        value="3"/>
                                                    <four
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Four"
                                                        value="4"/>
                                                </items>
                                            </numberOfSection>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <sectionOne
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section One"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sectionOneImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Image"
                                                fileNameParameter="./sectionOneImg"
												fileReferenceParameter="./sectionOneImgReference"
                                                mimeTypes="[image]"
                                                multiple="{Boolean}false"
                                                name="./sectionOneImage"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <sectionOneHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section One header text"
                                                fieldLabel="Section One Header"
                                                name="./sectionOneHeader"/>
                                            <sectionOneDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section One description text"
                                                fieldLabel="Section One Description"
                                                name="./sectionOneDescription"/>
                                            <sectionOneSuperScript
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter superscript for description."
                                                fieldLabel="Section One Superscript"
                                                name="./sectionOneSuperScript"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </sectionOne>
                    <sectionTwo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section Two"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sectionTwoImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Image"
                                                fileNameParameter="./sectionTwoImg"
												fileReferenceParameter="./sectionTwoImgReference"
                                                mimeTypes="[image]"
                                                multiple="{Boolean}false"
                                                name="./sectionTwoImage"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <sectionTwoHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section Two header text"
                                                fieldLabel="Section Two Header"
                                                name="./sectionTwoHeader"/>
                                            <sectionTwoDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section Two description text"
                                                fieldLabel="Section Two Description"
                                                name="./sectionTwoDescription"/>
                                            <sectionTwoSuperScript
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter superscript for description."
                                                fieldLabel="Section Two Superscript"
                                                name="./sectionTwoSuperScript"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </sectionTwo>
                    <sectionThree
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section Three"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sectionThreeImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Image"
                                                fileNameParameter="./sectionThreeImg"
												fileReferenceParameter="./sectionThreeImgReference"
                                                mimeTypes="[image]"
                                                multiple="{Boolean}false"
                                                name="./sectionThreeImage"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <sectionThreeHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section Three header text"
                                                fieldLabel="Section Three Header"
                                                name="./sectionThreeHeader"/>
                                            <sectionThreeDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section Three description text"
                                                fieldLabel="Section Three Description"
                                                name="./sectionThreeDescription"/>
                                            <sectionThreeSuperScript
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter superscript for description."
                                                fieldLabel="Section Three Superscript"
                                                name="./sectionThreeSuperScript"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </sectionThree>
                    <sectionFour
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section Four"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sectionFourImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Image"
                                                fileNameParameter="./sectionFourImg"
												fileReferenceParameter="./sectionFourImgReference"
                                                mimeTypes="[image]"
                                                multiple="{Boolean}false"
                                                name="./sectionFourImage"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <sectionFourHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section Four header text"
                                                fieldLabel="Section Four Header"
                                                name="./sectionFourHeader"/>
                                            <sectionFourDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter section Four description text"
                                                fieldLabel="Section Four Description"
                                                name="./sectionFourDescription"/>
                                            <sectionFourSuperScript
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter superscript for description."
                                                fieldLabel="Section Four Superscript"
                                                name="./sectionFourSuperScript"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </sectionFour>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
